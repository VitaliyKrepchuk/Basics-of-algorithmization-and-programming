#include <iostream>
using namespace std;
void main()
{
    int a[3][3], b[3][3], c[3][3], i, j;
    printf("Enter First Matrix: \n");
        for (i = 0; i < 3; i++)
        {
            for (j = 0; j < 3; j++)
            {
                cin >> a[i][j];
            }
        }
        printf("Enter Second Matrix: ");
        for (i = 0; i < 3; i++)
        {
            for (j = 0; j < 3; j++)
            {
                cin >> b[i][j];
            }
        }
        printf("\nAddition of Matrix is:  \n");
        for (i = 0; i < 3; i++)
        {
            printf("\n");
            for (j = 0; j < 3; j++)
            {
                c[i][j] = a[i][j] + b[i][j];
                printf("%d\t", c[i][j]);
            }
        }
}
