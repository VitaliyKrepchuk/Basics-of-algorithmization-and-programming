#include <iostream>
using namespace std;
void main()
{
    int a[10], i, sum = 0;
    float avg;
    for (i = 0; i < 10; i++)
    {
        printf("Enter Number %d : ", i + 1);
        cin >> a[i];
        sum = sum + a[i];
    }
    avg = sum / 10.0;
    printf ("\nSum =%d\nAverage =%f",sum,avg); 
}
