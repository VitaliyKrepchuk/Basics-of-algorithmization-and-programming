#include <iostream>
#include <stdio.h>
#include <string.h>
using namespace std;
void main()
{
    char s1[30], s2[30];
    printf("\nEnter The String (s1): ");
    cin >> s1;
    printf("\nEnter The String (s2): ");
    cin >> s2;
    strcat_s(s2, s1);
    printf("\nConcatenated String (s2) : %s", s2);
}
