#include <iostream>
#include <stdio.h>
#include <string.h>
using namespace std;
void main()
{
    char s1[30], s2[30];
    int cmp;
    printf("\nEnter The String (s1): ");
    cin >> s1;
    printf("\nEnter The String (s2): ");
    cin >> s2;
    cmp = strcmp(s1, s2);
    printf("\nComparison between s1 and s2 : %d", cmp);
}
