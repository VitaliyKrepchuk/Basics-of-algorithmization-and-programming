#include <iostream>
using namespace std;
void main()
{

    int a[10], i, n, c = 0;
    for (i = 0; i < 10; i++)
    {
        printf("Enter Number %d :", i + 1);
        cin >> a[i];
    }
    printf("\nEnter The digit to find:");
    cin >> n;
    for (i = 0; i < 10; i++)

    {
        if (a[i] == n)
        {
            c++;
        }
    }
    printf("%d occurs %d times", n, c);
}
